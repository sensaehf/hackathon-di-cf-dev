{
  "name": "cf-prep-webapp",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cf-prep/webapp",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/cf-prep/webapp"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/cf-prep/webapp"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "cf-prep-webapp:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "cf-prep-webapp:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "cf-prep-webapp:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "cf-prep-webapp:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/cf-prep/webapp/jest.config.ts"
      }
    },
    "codegen/frontend-client": {
      "executor": "nx:run-commands",
      "options": {
        "command": "grapqhl-codegen --config apps/cf-prep/webapp/codegen.yml"
      },
      "dependsOn": [
        {
          "projects": ["cf-prep-gql"],
          "target": "codegen/backend-schema"
        }
      ],
      "inputs": [
        "default",
        "{workspaceRoot}/apps/cf-prep/gql/src/api.graphql"
      ],
      "outputs": [
        "{projectRoot}/graphql/schema.tsx",
        "{projectRoot}/graphql/possibleTypes.json"
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
